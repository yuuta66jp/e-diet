<h1>edit</h1>
<%= form_with model: @meal_record do |f| %>

  <h3>食事画像</h3>
  <%= f.attachment_field :meal_image %>

  <h3>タイトル</h3>
  <%= f.text_field :title %>


  <h3>本文</h3>
  <%= f.text_area :body %>

  <h3>いつ食べましたか</h3>
  <%= f.select :intake_status, MealRecord.intake_statuses.keys.to_a, prompt: "選択してください" %>

  <%= f.submit "編集する" %>

<% end %>

